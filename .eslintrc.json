{
  "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },

  "rules": {
    "import/extensions": "off",
    "node/no-unsupported-features/es-syntax": "off",
    "no-undef": "off",
    "prefer-destructuring": "off",
    "class-methods-use-this": "off"
  }
}
